[project]
title = Voronoi
version = 0.1
dependencies = https://github.com/andsve/dirtylarry/archive/master.zip

[bootstrap]
main_collection = /main/main.collectionc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 800
height = 600

[physics]
scale = 0.02

[script]
shared_state = 1

[library]
include_dirs = voronoi

[osx]
bundle_identifier = com.jcash.voronoi

[android]
package = com.jcash.voronoi

[ios]
bundle_identifier = com.jcash.voronoi
